{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": "off",
    "prefer-arrow-callback":"off",
    "no-unused-vars":"off",
    "eol-last":"off",
    "linebreak-style":"off",
    "indent":"off",
    "consistent-return":"off",
    "no-param-reassign":"off",
    "radix":"off",
    "dot-notation":"off",
    "no-plusplus":"off",
    "import/no-dynamic-require":"off",
    "global-require":"off",
    "prefer-destructuring":"off",
    "no-console":"off",
    "import/no-mutable-exports":"off",
    "camelcase":"off",
    "import/prefer-default-export":"off"
  }
}